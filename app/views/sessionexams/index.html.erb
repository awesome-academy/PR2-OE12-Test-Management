<% provide(:title, t(".All" ))%>
<h1><%= t ".All" %> </h1>

<%= search_form_for @q do |f| %>
  <%= f.label :exam_name_or_period_for_or_supervisor_cont %>
  <%= f.search_field :exam_name_or_period_for_or_supervisor_cont %>
  <%= f.submit %>
<% end %>

<table>
  <tr>
    <th><%=t ".exam_name"%></th>
    <th><%=t ".period_for"%></th>
    <th><%=t ".supervisor"%></th>
    <th><%= t ".create_at"%></th>
    <th><%=t ".updated_at"%></th>
    <th>
      <%=t ".View"%>
    </th>
    <th><%= t ".Edit"%></th>
    <th><%=t ".Remove"%></th>
  </tr>
  <% @sessionexams.each do |sessionexam| %>
  <tr>
    <td>
      <%= sessionexam.exam_name %>
    </td>
    <td>
      <%= sessionexam.period_for %>
    </td>
    <td>
      <%= sessionexam.supervisor %>
    </td>
    <td>
      <%= sessionexam.create_at %>
    </td>
    <td>
      <%= sessionexam.updated_at %>
    </td>
    <td>
      <%= link_to t(".View"), sessionexam_path(sessionexam) %>
    </td>
    <td>
      <%= button_to t(".Edit"), edit_sessionexam_path(sessionexam), method: "get"%>
    </td>
    <td>
      <%= button_to t(".Remove"), sessionexam_path(sessionexam), method: "delete", data: { confirm: t(".Are") } %>
    </td>
    <% end %>
</table>
<%= link_to t(".Add"), new_sessionexam_path %>
